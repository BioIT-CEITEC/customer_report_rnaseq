{
  "workflow_description": {
    "name": "customer_report_RNA",
    "version": 1.0,
    "label": "Customer Report for RNA-Seq and DE analysis",
    "type": "customer_report",
    "inputs": [
      "qc_reports/*",
      "results/*"
    ],
    "outputs": [
      "customer_report/*",
      "logs/*"
    ],
    "report_index": "customer_report/customer_report.html",
    "reports":[
      "customer_report/customer_report.html"
    ]
  },
  "general_params": [
    "entity_name",
    "sample_name",
    "condition",
    "replicate"
  ],
  "requested_params": [
    "is_paired",
    "feature_count",
    "RSEM",
    "reference",
    "UMI",
    "RNAseq_type",
    "type_of_extraction",
    "strandness"
  ],
  "gui_params": {
    "primary": {
      "author": {
        "label": "Author name",
        "type": "string",
        "default": "Nicolas Blavet"
      },
      "email": {
        "label": "Author email",
        "type": "string",
        "default": "nicolas.blavet@ceitec.muni.cz"
      },
      "conditions_to_compare": {
        "label": "Conditions to compare",
        "type": "string",
        "default": "all"
      },
      "biotypes": {
        "label": "Biotypes",
        "type": "string",
        "default": "all"
      }
    },
    "detailed": {
      "enrich_padj": {
        "label": "Enrichment adjusted p-value cut-off",
        "type": "string",
        "default": "0.1"
      },
      "enrich_padjmethod": {
        "label": "Enrichment adjusted p-value method",
        "type": "enum",
        "default": "BH",
        "list": {
          "BH": "BH",
          "BY": "BY",
          "fdr": "fdr",
          "holm": "holm",
          "hochberg": "hochberg",
          "hommel": "hommel",
          "bonferroni": "bonferroni",
          "none": "none"
        }
      },
      "enrich_minGSSize": {
        "label": "Enrichment minimal size of genes annotated",
        "type": "string",
        "default": "2"
      },
      "enrich_maxGSSize": {
        "label": "Enrichment maximal size of genes annotated",
        "type": "string",
        "default": "Inf"
      },
      "gsea_padj": {
        "label": "GSEA adjusted p-value cut-off",
        "type": "string",
        "default": "0.1"
      },
      "gsea_padjmethod": {
        "label": "GSEA adjusted p-value method",
        "type": "enum",
        "default": "BH",
        "list": {
          "BH": "BH",
          "BY": "BY",
          "fdr": "fdr",
          "holm": "holm",
          "hochberg": "hochberg",
          "hommel": "hommel",
          "bonferroni": "bonferroni",
          "none": "none"
        }
      },
      "gsea_minGSSize": {
        "label": "GSEA minimal size of genes annotated",
        "type": "string",
        "default": "2"
      },
      "gsea_maxGSSize": {
        "label": "GSEA maximal size of genes annotated",
        "type": "string",
        "default": "Inf"
      },
      "gsea_eps": {
        "label": "GSEA boundary for p-value calculation",
        "type": "string",
        "default": "0"
      },
      "gsea_nPermSimple": {
        "label": "GSEA Number of permutations for preliminary estimation of p-values",
        "type": "string",
        "default": "10000"
      },
      "gsea_by": {
        "label": "GSEA Number of permutations for preliminary estimation of p-values",
        "type": "enum",
        "default": "fgsea",
        "list": {
          "fgsea": "fgsea",
          "DOSE": "DOSE"
        }
      }
    },
    "samples": {
      "condition": {
        "label": "Condition",
        "type": "string",
        "default": ""
      },
      "replicate": {
        "label": "Replicate ID",
        "type": "string",
        "default": ""
      }
    }
  }
}