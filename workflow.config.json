{
  "workflow_description": {
    "name": "customer_report_RNA",
    "version": 2.0,
    "label": "Customer Report for RNA-Seq and DE analysis",
    "type": "customer_report",
    "run_parameters": {
      "cpu": 2,
      "memory": 65
    },
    "inputs": [
      "qc_reports/*"
    ],
    "inputs_optional":[
      "DE_RSEM/*",
      "DE_featureCount_exon/*",
      "DE_featureCount_gene/*",
      "DE_featureCount_transcript/*",
      "DE_featureCount_3pUTR/*",
      "DE_featureCount_5pUTR/*",
      "DE_salmon_map/*",
      "DE_salmon_align/*",
      "DE_kallisto/*"
    ],
    "outputs": [
      "customer_report/*",
      "logs/*"
    ],
    "report_index": "customer_report/customer_report.html",
    "reports":[
      "customer_report/customer_report.html"
    ]
  },
  "general_params": [
    "entity_name",
    "sample_name"
  ],
  "requested_params": [
    "is_paired",
    "reference",
    "keep_not_compared_samples_for_normalization",
    "featureCount",
    "count_over",
    "RSEM",
    "kallisto",
    "salmon_align",
    "salmon_map",
    "biobloom",
    "UMI",
    "RNAseq_type",
    "type_of_extraction",
    "strandness",
    "qc_samtools",
    "qc_dupradar_RNA",
    "qc_qualimap_RNA",
    "qc_picard_RNA",
    "qc_biotypes_RNA",
    "qc_fastq_screen_RNA",
    "qc_RSeQC_RNA"
  ],
  "gui_params": {
    "primary": {
      "author": {
        "label": "Author name",
        "type": "string",
        "default": "Nicolas Blavet"
      },
      "email": {
        "label": "Author email",
        "type": "string",
        "default": "nicolas.blavet@ceitec.muni.cz"
      },
      "conditions_to_compare": {
        "label": "Conditions to compare",
        "type": "string",
        "default": "all"
      },
      "keep_not_compared_samples_for_normalization": {
        "label": "Keep not compared samples for normalization.",
        "type": "bool",
        "default": true
      }
    },
    "detailed": {
      "biotypes": {
        "label": "Biotypes",
        "type": "enum",
        "default": "all",
        "list": {"all": "all"}
      },
      "display_samtools": {
        "label": "Report QC samtools",
        "type": "bool",
        "default": false
      },
      "display_dupradar_RNA": {
        "label": "Report QC dupradar RNA",
        "type": "bool",
        "default": true
      },
      "display_qualimap_RNA": {
        "label": "Report QC qualimap RNA",
        "type": "bool",
        "default": true
      },
      "display_picard_RNA": {
        "label": "Report QC picard RNA",
        "type": "bool",
        "default": true
      },
      "display_biotypes_RNA": {
        "label": "Report QC biotypes RNA",
        "type": "bool",
        "default": true
      },
      "display_fastq_screen_RNA": {
        "label": "Report QC fastq screen RNA",
        "type": "bool",
        "default": true
      },
      "display_RSeQC_RNA": {
        "label": "Report QC RSeQC RNA",
        "type": "bool",
        "default": true
      },
      "display_biobloom": {
        "label": "Report Biobloom tools",
        "type": "bool",
        "default": true
      }
    },
    "samples": {
      "condition": {
        "label": "Condition",
        "type": "string",
        "default": ""
      },
      "replicate": {
        "label": "Replicate ID",
        "type": "string",
        "default": ""
      }
    }
  }
}