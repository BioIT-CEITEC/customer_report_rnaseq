{
  "workflow_description": {
    "name": "customer_report_RNA",
    "version": 1.0,
    "label": "Customer Report for RNA-Seq and DE analysis",
    "type": "customer_report",
    "inputs": [
      "qc_reports/*",
      "results/*"
    ],
    "outputs": [
      "customer_report/*",
      "logs/*"
    ],
    "report_index": "customer_report/customer_report.html",
    "reports":[
      "customer_report/customer_report.html"
    ]
  },
  "general_params": [
    "entity_name",
    "sample_name"
  ],
  "requested_params": [
    "is_paired",
    "reference",
    "feature_count",
    "RSEM",
    "salmon",
    "kallisto",
    "UMI",
    "RNAseq_type",
    "type_of_extraction",
    "strandness",
    "qc_samtools",
    "qc_dupradar_RNA",
    "qc_qualimap_RNA",
    "qc_picard_RNA",
    "qc_biotypes_RNA",
    "qc_fastq_screen_RNA",
    "qc_RSeQC_RNA",
    "biobloom"
  ],
  "gui_params": {
    "primary": {
      "author": {
        "label": "Author name",
        "type": "string",
        "default": "Nicolas Blavet"
      },
      "email": {
        "label": "Author email",
        "type": "string",
        "default": "nicolas.blavet@ceitec.muni.cz"
      },
      "conditions_to_compare": {
        "label": "Conditions to compare",
        "type": "string",
        "default": "all"
      },
      "biotypes": {
        "label": "Biotypes",
        "type": "string",
        "default": "all"
      }
    },
    "detailed": {
      "qc_samtools_report": {
        "label": "Report QC samtools",
        "type": "bool",
        "default": true
      },
      "qc_dupradar_RNA_report": {
        "label": "Report QC dupradar RNA",
        "type": "bool",
        "default": true
      },
      "qc_qualimap_RNA_report": {
        "label": "Report QC qualimap RNA",
        "type": "bool",
        "default": true
      },
      "qc_picard_RNA_report": {
        "label": "Report QC picard RNA",
        "type": "bool",
        "default": true
      },
      "qc_biotypes_RNA_report": {
        "label": "Report QC biotypes RNA",
        "type": "bool",
        "default": true
      },
      "qc_fastq_screen_RNA_report": {
        "label": "Report QC fastq screen RNA",
        "type": "bool",
        "default": true
      },
      "qc_RSeQC_RNA_report": {
        "label": "Report QC RSeQC RNA",
        "type": "bool",
        "default": true
      },
      "biobloom_report": {
        "label": "Report Biobloom tools",
        "type": "bool",
        "default": false
      }
    },
    "samples": {
      "condition": {
        "label": "Condition",
        "type": "string",
        "default": ""
      },
      "replicate": {
        "label": "Replicate ID",
        "type": "string",
        "default": ""
      }
    }
  }
}