{
  "workflow_description": {
    "name": "customer_report_RNA",
    "version": 1.0,
    "label": "Customer Report for RNA-Seq and DE analysis",
    "type": "customer_report",
    "inputs": [
      "qc_reports/*",
      "results/*"
    ],
    "outputs": [
      "customer_report/*",
      "logs/*"
    ],
    "report_index": "customer_report/customer_report.html",
    "reports":[
      "customer_report/customer_report.html"
    ]
  },
  "general_params": [
    "entity_name",
    "sample_name",
    "condition",
    "replicate"
  ],
  "requested_params": [
    "is_paired",
    "feature_count",
    "RSEM",
    "reference",
    "UMI",
    "RNAseq_type",
    "type_of_extraction",
    "strandness"
  ],
  "gui_params": {
    "primary": {
      "author": {
        "label": "Author name",
        "type": "string",
        "default": "Nicolas Blavet"
      },
      "email": {
        "label": "Author email",
        "type": "string",
        "default": "nicolas.blavet@ceitec.muni.cz"
      },
      "conditions_to_compare": {
        "label": "Conditions to compare",
        "type": "string",
        "default": "all"
      },
      "biotypes": {
        "label": "Biotypes",
        "type": "string",
        "default": "all"
      }
    },
    "detailed": {},
    "samples": {
      "condition": {
        "label": "Condition",
        "type": "string",
        "default": ""
      },
      "replicate": {
        "label": "Replicate ID",
        "type": "string",
        "default": ""
      }
    }
  }
}